.MODEL SMALL
.STACK 100H

 .DATA
    low  DB  'LOWERCASE: $'
    up  DB  'UPPERCASE: $'

 .CODE
   MAIN PROC
     MOV AX, @DATA                
     MOV DS, AX

     LEA DX, low           
     MOV AH, 9
     INT 21H

     MOV AH, 1                    
     INT 21H

     MOV BL, AL                  

     MOV AH, 2                   
     MOV DL, 0DH
     INT 21H 
     
     
     MOV DL, 0AH            
     INT 21H

     LEA DX, up         
     MOV AH, 9
     INT 21H

     SUB BL, 32                   

     MOV AH, 2                    
     MOV DL, BL     
     INT 21H
     
     MOV AH, 4CH                
     INT 21H
   MAIN ENDP
 END MAIN

